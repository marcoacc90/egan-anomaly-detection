FROM tensorflow/tensorflow:latest-gpu-py3

COPY Dataset3/normalTraining/ Dataset3/normalTraining/
COPY Modules/ Modules/
COPY AETraining.py AETraining.py

RUN python3 -m pip install matplotlib
RUN python3 -m pip install sklearn
RUN python3 -m pip install pillow

# Execute the program
CMD python3 AETraining.py Dataset3/normalTraining/  > E500_AE/training.txt
